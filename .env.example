# Optional proxies (comma-separated, will rotate) e.g. http://user:pass@host:port
PROXIES=

# Default output directory
OUTPUT_DIR=data

# Max parallel requests (be nice and throttle politely)
CONCURRENCY=8

# Delay (seconds) between requests to the same domain
DELAY_SECONDS=1.0

# User-Agent to use; if empty a randomized UA will be used
USER_AGENT=
